{
  "compilerOptions": {
    "allowJs": true,
    "alwaysStrict": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "checkJs": true,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "jsx": "react-jsxdev",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "paths": {
      "react-native": [
        "node_modules/react-native-web"
      ]
    },
    "strict": true,
    "outDir": "dist",
    "module": "ESNext",
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "sourceMap": true,
    "target": "ESNext",
    "types": [
      "vite/client",
    ]
  },
  "include": [
    "server.ts",
  ],
  "exclude": [
    "node_modules",
  ],
  "ts-node": {
    "transpileOnly": true,
    "compilerOptions": {
      "module": "CommonJS"
    }
  }
}